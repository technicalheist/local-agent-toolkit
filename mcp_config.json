{
    "mcpServers": {
        "playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest"
            ],
            "description": "Playwright MCP server for web automation and browser control",
            "disabled": true
        },
        "filesystem": {
            "command": "npx",
            "args": [
                "@modelcontextprotocol/server-filesystem",
                "./"
            ],
            "description": "Filesystem MCP server for file operations",
            "disabled": true
        },
        "sqlite": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "-v",
                "mcp-sqlite:/mcp",
                "mcp/sqlite",
                "--db-path",
                "/mcp/db.sqlite"
            ],
            "description": "SQLite MCP server for database operations using Docker",
            "disabled": true
        }
    }
}